{
  "name": "lilademarcq",
  "scripts": {
    "build": "next build && yarn build:post",
    "build:post": "yarn build:post:remove-source-maps && yarn build:post:build-sitemap",
    "build:post:build-sitemap": "next-sitemap",
    "build:post:remove-source-maps": "find .next -type f -name \"*.map\" -exec rm {} \\;",
    "clean": "npx rimraf .next",
    "dev": "yarn clean && next dev",
    "export": "next export",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "package:clean": "rimraf .next && rimraf node_modules",
    "package:reinstall": "yarn package:clean && yarn",
    "package:sort": "npx sort-package-json",
    "start": "next start",
    "test": "yarn jest",
    "test:coverage": "jest --coverage && codecov",
    "test:experimental": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "jest --watch",
    "test:watch:all": "jest --watchAll"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^15.12.1",
    "@fontsource/montserrat": "^4.5.7",
    "@fontsource/spectral": "^4.5.5",
    "@plaiceholder/next": "^2.3.0",
    "file-loader": "^6.2.0",
    "gray-matter": "^4.0.3",
    "jotai": "^1.6.4",
    "next": "^12.1.5",
    "photoswipe": "^5.2.4",
    "plaiceholder": "^2.3.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-player": "^2.10.0",
    "react-use-measure": "^2.1.1",
    "remark": "^14.0.2",
    "remark-html": "^15.0.1",
    "sharp": "^0.30.4",
    "styled-components": "^5.3.5",
    "vfile": "^5.3.2"
  },
  "devDependencies": {
    "@bamdadsabbagh/eslint-config": "^2.0.25",
    "@contentful/rich-text-types": "^15.12.1",
    "@iconify/icons-entypo-social": "^1.2.2",
    "@iconify/icons-fa6-brands": "^1.2.2",
    "@iconify/react": "^3.2.1",
    "@types/photoswipe": "^4.1.2",
    "@types/react": "18.0.8",
    "@types/react-dom": "^18.0.0",
    "@types/styled-components": "^5.1.25",
    "babel-plugin-styled-components": "^2.0.7",
    "eslint-config-next": "^12.1.5",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-testing-library": "^5.3.1",
    "next-sitemap": "^2.5.20",
    "null-loader": "^4.0.1",
    "rimraf": "^3.0.2",
    "sass": "^1.51.0",
    "sass-reset": "^1.0.4",
    "typescript-plugin-styled-components": "^2.0.0"
  },
  "version": "1.20.1"
}
