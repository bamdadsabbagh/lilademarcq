{
  "name": "lilademarcq",
  "scripts": {
    "build": "next build && pnpm build:post",
    "build:post": "pnpm build:post:remove-source-maps && pnpm build:post:build-sitemap",
    "build:post:build-sitemap": "next-sitemap",
    "build:post:remove-source-maps": "find .next -type f -name \"*.map\" -exec rm {} \\;",
    "clean": "npx rimraf .next",
    "dev": "pnpm clean && next dev",
    "export": "next export",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "package:clean": "npx rimraf .next && npx rimraf node_modules",
    "package:reinstall": "pnpm package:clean && pnpm i",
    "package:sort": "npx sort-package-json",
    "start": "next start",
    "test": "pnpm jest",
    "test:coverage": "jest --coverage && codecov",
    "test:experimental": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "jest --watch",
    "test:watch:all": "jest --watchAll"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^15.12.1",
    "@fontsource/montserrat": "^4.5.10",
    "@fontsource/spectral": "^4.5.8",
    "@plaiceholder/next": "^2.3.0",
    "embla-carousel-autoplay": "^6.2.0",
    "embla-carousel-react": "^6.2.0",
    "file-loader": "^6.2.0",
    "jotai": "^1.6.6",
    "just-shuffle": "^4.0.1",
    "next": "^12.1.6",
    "next-seo": "^5.4.0",
    "photoswipe": "^5.2.7",
    "plaiceholder": "^2.3.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-player": "^2.10.1",
    "react-use-measure": "^2.1.1",
    "sharp": "^0.30.4",
    "styled-components": "^5.3.5",
    "vfile": "^5.3.2"
  },
  "devDependencies": {
    "@bamdadsabbagh/eslint-config": "^2.0.26",
    "@contentful/rich-text-types": "^15.12.1",
    "@iconify/icons-entypo-social": "^1.2.2",
    "@iconify/icons-fa6-brands": "^1.2.2",
    "@iconify/react": "^3.2.1",
    "@types/grecaptcha": "^3.0.4",
    "@types/photoswipe": "^4.1.2",
    "@types/react": "18.0.9",
    "@types/react-dom": "^18.0.4",
    "@types/styled-components": "^5.1.25",
    "babel-plugin-styled-components": "^2.0.7",
    "eslint-config-next": "^12.3.4",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-testing-library": "^5.11.1",
    "next-sitemap": "^2.5.20",
    "null-loader": "^4.0.1",
    "prettier": "^3.1.1",
    "rimraf": "^3.0.2",
    "sass": "^1.51.0",
    "sass-reset": "^1.0.4",
    "typescript-plugin-styled-components": "^2.0.0"
  },
  "version": "1.36.7"
}
